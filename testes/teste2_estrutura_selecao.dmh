## Declarando variáveis ##
var a = 4.32;
var b = 3;
var value = 0;

## Utilizando estrutura de repetição(if..else) ##
if (a >= b) do {
	show(a);
    value = sen(60) * 12 - 321 // 213;
}
else do {
	show(b);
    value = cos(60) * 12 - 321 // 215;
};

## Printando value e o arco tangente de value ##
show(value);
show(arctang(value));